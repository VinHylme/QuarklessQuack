{
  "Logging": {
	"LogLevel": {
	  "Default": "Information"
	}
  },
  "MaxConcurrentRequests": {
	"Enabled": true,
	"Limit": "#{MAX_CONCURRENT_REQUEST_LIMIT}#",
	"MaxQueueLength": "#{MAX_CONCURRENT_REQUEST_QUEUE_LENGTH}#",
	"MaxTimeInQueue": "#{MAX_CONCURRENT_REQUEST_TIME_IN_QUEUE}#",
	"LimitExceededPolicy": "FifoQueueDropTail",
	"ExcludePaths": [
	  "/error",
	  "/css",
	  "/js",
	  "/dist"
	]
  },
  "IpRateLimiting": {
	"EnableEndpointRateLimiting": false,
	"StackBlockedRequests": false,
	"RealIpHeader": "X-Original-For",
	"ClientIdHeader": "X-Client-ID",
	"HttpStatusCode": 429,
	"IpWhitelist": [ "::1/10" ],
	"EndpointWhitelist": [ "*:/api/" ],
	"ClientWhitelist": [ "x-key-1-v" ],
	"GeneralRules": [
	  {
		"Endpoint": "*",
		"Period": "1s",
		"Limit": 200
	  },
	  {
		"Endpoint": "*",
		"Period": "15m",
		"Limit": 100
	  },
	  {
		"Endpoint": "*",
		"Period": "12h",
		"Limit": 1000
	  },
	  {
		"Endpoint": "*",
		"Period": "7d",
		"Limit": 10000
	  }
	]
  },
  "IpRateLimitPolicies": {
	"IpRules": []
  },
  "AppS3Bucket": "#{S3Bucket}#",
  "AWSCredential": {
	"PoolID": "#{AWS_POOL_ID}#",
	"AppClientID": "#{AWS_APP_CLIENT_ID}#",
	"AppSecretKey": "#{AWS_APP_SECRET_KEY}#",
	"AuthUrl": "#{AWS_AUTH_URL}#"
  },
  "AWS": {
	"Region": "#{AWS_REGION}#",
	"AccessKey": "#{AWS_ACCESS_KEY}#",
	"SecretKey": "#{AWS_SECRET_KEY}#"
  },
  "ConnectionStrings": {
	"Redis": "#{REDIS_CON_STRING}#",
	"MongoClientStrings": "{DB_CON_STRING}",
	"DatabaseNames": {
	  "Accounts": "#{DB_ACC}#",
	  "Control": "#{DB_CON}#",
	  "Content": "#{DB_CA}#",
	  "Scheduler": "#{DB_SC}"
	}
  },
  "APIServices": {
	"Yandex": "#{YANDEX_LOCATION}#",
	"DetectLanguageAPI": "#{DETECT_LANG_API_KEY}#",
	"NaturalLanguageProcessing": "#{NATURAL_LANG_API_ENDPOINT}#"
  },
  "Endpoints": {
	"ImageSearchEndpointGoogle": "#{IM_SEARCH_API_ENDPOINT}#",
	"FrontEnd": "#{FRONT_ENDPOINT}#"
  },
  "MediaPath": {
	"videosTempPath": "#{VID_TEMP_FILE_LOCATION}#",
	"imagesTempPath": "#{IMG_TEMP_FILE_LOCATION}#",
	"enginePath": "#{FFMPEG_ENGINE_LOCATION}#"
  },
  "Stripe": {
	"PublishableKey": "#{SP_KEY}#",
	"SecretKey": "#{SP_SECRET}#",
	"WebHookSecret": "#{SP_WEB_SECRET}#"
  }
}